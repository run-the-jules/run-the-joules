<%= form_with url: users_friends_search_path, method: :get, local: true do |f| %>
    <%= f.text_field :query, value: "email", :style => "border: solid; border-style: solid; border-color: rgba(209, 213, 219);" %>
    <%= f.submit "Follow Friend", class: "button rounded-lg bg-gray-300 px-8 " %>
<% end %>
